#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    float x, y;
   printf("Enter x= ");
   scanf("%f", &x);
   printf("Enter y= ");
   scanf("%f", &y);

   if ((x<0 && y<0 && x*x+y*y<1) || ( x>0 && y>0 && x*x+y*y<1 && x>y*y) || (x>0 && y<0 && x*x+y*y>1 && x>y*y))
   {
       printf("This point fall into");
   }
   else {
    printf("This point does't fall into!!!");
   }
    return 0;
}
